{

    "name": "Codechef-Notifier",
    "version": "0.0.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "logo.png" 
    },

    "icons": {
        "128": "logo.png"
    },

    "permissions": ["activeTab"],
    
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }

}
